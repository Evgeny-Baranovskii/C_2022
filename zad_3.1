#include <iostream>
using namespace std;

int main()
{
    char b[27]={"ABCDEFGHIJKLMNOPQRSTYVWXYZ"};
    int n;
    cout<<"Введите число n: ";
    cin>>n;
    if(n<=24)
    {
        for(int i=0;i<n;i++)
        {
            cout<<b[i]<<" ";
        }
    }
    else
    {
        cout<<"Введено большое число. Попробуте ещё раз";
    }
     return 0;
}
